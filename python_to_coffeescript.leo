def parse_group(group):
    if len(group) &gt;= 3 and group[-2] == 'as':
        del group[-2:]
    ndots = 0
    i = 0
    while len(group) &gt; i and group[i].startswith('.'):
        ndots += len(group[i])
        i += 1
    assert ''.join(group[: i]) == '.' * ndots, group
    del group[: i]
    assert all(g == '.' for g in group[1:: 2]), group
    return ndots, os.sep.join(group[:: 2])
